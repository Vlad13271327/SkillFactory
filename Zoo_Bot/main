    else:
        animal_info = totem_info[tuple(sorted(max_categories))]
        animal_sticker = totem_info[tuple(sorted(max_categories))][1]
        bot.send_message(message.chat.id, animal_info)
        bot.send_sticker(message.chat.id, animal_sticker)
        final_choice(message)

    return animal_info, animal_sticker


